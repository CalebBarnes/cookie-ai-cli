{
  "name": "cookie-ai-cli",
  "description": "A command-line interface tool designed to bridge the gap between natural language processing and command-line operations.",
  "version": "1.4.4-beta.1",
  "type": "module",
  "bin": {
    "ai": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "repository": "github:CalebBarnes/cookie-ai-cli",
  "author": "Caleb Barnes <https://github.com/CalebBarnes>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "postbuild": "cp src/ai-response-schema.ts dist/",
    "build:watch": "tsc -w & npm run copy-ts-file",
    "start": "node ./dist/index.js",
    "start:ip": "node ./dist/index.js what is my ip --debug",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@vitest/ui": "^1.3.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "keywords": [
    "ai",
    "cli",
    "command line tool",
    "openai",
    "openai api",
    "GPT-4",
    "GPT-3",
    "GPT"
  ]
}
