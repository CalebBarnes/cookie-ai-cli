{
  "name": "cookie-ai-cli",
  "description": "A command-line interface tool designed to bridge the gap between natural language processing and command-line operations.",
  "version": "1.4.4-beta.2",
  "type": "module",
  "bin": {
    "ai": "./dist/index"
  },
  "files": [
    "dist"
  ],
  "repository": "github:CalebBarnes/cookie-ai-cli",
  "author": "Caleb Barnes <https://github.com/CalebBarnes>",
  "license": "MIT",
  "scripts": {
    "build": "tsc && pnpm run copy-ts-file",
    "copy-ts-file": "cp src/ai-response-schema.ts dist/",
    "build:watch": "tsc -w",
    "start": "node --no-warnings=ExperimentalWarning ./dist/index",
    "start:help": "node --no-warnings=ExperimentalWarning ./dist/index files --help",
    "start:version": "node --no-warnings=ExperimentalWarning ./dist/index --version",
    "start:debug": "node --no-warnings=ExperimentalWarning ./dist/index --debug",
    "start:ip": "pnpm start what is my ip",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "inquirer": "^9.2.15",
    "logform": "^2.6.0",
    "ora": "^8.0.1",
    "winston": "^3.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.24",
    "@vercel/style-guide": "^6.0.0",
    "@vitest/ui": "^1.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-require-extensions": "^0.1.3",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "keywords": [
    "ai",
    "cli",
    "command line tool",
    "openai",
    "openai api",
    "GPT-4",
    "GPT-3",
    "GPT"
  ]
}
